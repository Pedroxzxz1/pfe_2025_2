def pode_usar_bicicleta_sem_capacete():

    idade = int(input("Informe sua idade: "))

    tem_cnh = input("VocÃª possui CNH? (s/n): ").strip().lower()

    # LÃ³gica NAND

    maior_de_idade = idade >= 18

    possui_cnh = tem_cnh == 's'

    if not (maior_de_idade and possui_cnh):
        print("ğŸŸ¢ VocÃª pode andar de bicicleta elÃ©trica sem capacete.")
    else:
        print("ğŸ”´ VocÃª NÃƒO pode andar de bicicleta elÃ©trica sem capacete.")

